@{
}
@model IEnumerable<KCT.Models.StudentViewModel>

<h2>All Students</h2>

<table class="table">
    <thead>
        <tr>
            <th>Full Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.FullName</td>
                <td>@student.Email</td>
            </tr>
        }
    </tbody>
</table>

<a href="@Url.Action("Create", "DStudent")" class="btn btn-primary">Add New Student</a>
